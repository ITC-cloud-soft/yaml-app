@model Yaml.Infrastructure.Dto.YamlClusterInfoDto
apiVersion: v1
kind: PersistentVolumeClaim
metadata:
  name: @Model.ClusterName-pvc
  namespace: @Model.AppName
spec:
  accessModes:
    - ReadWriteOnce
  storageClassName: @Model.Disk?.Type
  resources:
    requests:
      storage: @Model.Disk?.Size