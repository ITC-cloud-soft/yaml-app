@model Yaml.Infrastructure.Dto.YamlClusterInfoDto
apiVersion: apps/v1
kind: Deployment
metadata:
  name: @Model.ClusterName-app
spec:
  replicas: @Model.PodNum
  selector:
    matchLabels:
      app: web
  template:
    metadata:
      labels:
        app: web
    spec:
      containers:
        - name: a
          image: @Model.Image
          ports: 
            - containerPort: 80
        - name: flaskapp-demo
          image: jcdemo/flaskapp
          ports:
            - containerPort: 5000