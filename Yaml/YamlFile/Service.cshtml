@model Yaml.Infrastructure.Dto.YamlClusterInfoDto
apiVersion: v1
kind: Service
metadata:
  name: @Model.ClusterName-svc
  namespace: @Model.AppName-ns
spec:
  selector:
    app:  @Model.ClusterName-app
  ports:
    - protocol: TCP
      port: @Model.Port
      targetPort: @Model.TargetPort
  type: ClusterIP